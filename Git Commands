git init
git config --global user.name "user name"
git config --global user.email "username@gmail.com"
git remote add origin https://github.com/selvaswe/couponservice.git
git remote add origin https://<<TOKEN>@github.com/selvaswe/couponservice.git


echo "# productservice" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/selvaswe/productservice.git
git push -u origin main
